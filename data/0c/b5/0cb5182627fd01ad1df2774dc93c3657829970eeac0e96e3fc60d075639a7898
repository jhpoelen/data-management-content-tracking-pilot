jQuery(document).ready(function($) {
  $(window).on("scroll", function(e) {
    if (window.matchMedia("(max-width: 719px)").matches) {
      var navmobile = $('#nav-mobile');
    } else {
      var navmobile = $('#nav-topbar');
    }
    if (navmobile.length) {
      // if topbar exists
      var position = navmobile.position(); 
      var openbarreheight=$('#ob-header').height();
//    if (window.matchMedia("(max-width: 720px)").matches) {
      if (navmobile.position().top < $(window).scrollTop()-openbarreheight ) {
        navmobile.css({
          position: 'fixed',
          top: '0px'
        });
      } else if (position.top > $(window).scrollTop()-openbarreheight && navmobile.css('position') == 'fixed') {
        navmobile.css({
          position: 'absolute'
        });
      }
//    }
  // remove the style attribute added by the previous function on window resize
/*
    $(window).on('resize', function(){
